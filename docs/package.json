  "scripts": {
    "copy-hexagon:docs": "cpx \"node_modules/hexagon-js/dist/**/*\" resources/hexagon/docs/ --clean",
    "postcopy-hexagon:docs": "node build/docs-hexagon.js",
    "copy-hexagon:version": "cpx \"node_modules/hexagon-js/dist/**/*\" resources/hexagon/2.0.2/ --clean",
