    "quantum-code-highlight": "^2.0.0",
    "quantum-core": "^2.0.1",
    "quantum-docs": "^2.0.0",
    "quantum-dom": "^2.0.0",
    "quantum-html": "^2.1.0",
    "quantum-template": "^2.0.0",
    "quantum-version": "^2.1.0"
  }
