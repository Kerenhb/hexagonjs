@inline ../../../../shared/common.um
@titlebar Color Picker Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Color Picker Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/color-picker/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/color-picker/