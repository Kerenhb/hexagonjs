@inline ../../../../shared/common.um
@titlebar Progress Bar Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Progress Bar Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/progress-bar/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/progress-bar/