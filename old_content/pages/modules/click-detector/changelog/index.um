@inline ../../../../shared/common.um
@titlebar Click Detector Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Click Detector Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/click-detector/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/click-detector/