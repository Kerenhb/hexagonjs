@inline ../../../../shared/common.um
@titlebar Drag Container Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Drag Container Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/drag-container/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/drag-container/