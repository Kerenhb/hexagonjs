@inline ../../../../shared/common.um
@titlebar Inline Editable Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Inline Editable Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/inline-editable/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/inline-editable/