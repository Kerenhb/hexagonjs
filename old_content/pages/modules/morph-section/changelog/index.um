@inline ../../../../shared/common.um
@titlebar Morph Section Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Morph Section Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/morph-section/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/morph-section/