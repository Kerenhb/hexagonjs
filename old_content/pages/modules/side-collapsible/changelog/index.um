@inline ../../../../shared/common.um
@titlebar Side Collapsible Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Side Collapsible Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/side-collapsible/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/side-collapsible/