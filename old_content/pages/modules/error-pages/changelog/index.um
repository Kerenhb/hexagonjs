@inline ../../../../shared/common.um
@titlebar Error Pages Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Error Pages Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/error-pages/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/error-pages/