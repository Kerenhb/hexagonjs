@inline ../../../../shared/common.um
@titlebar Tag Input Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Tag Input Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/tag-input/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/tag-input/