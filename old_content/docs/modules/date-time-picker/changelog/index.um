@inline ../../../../shared/common.um
@titlebar Date Time Picker Changelog

@div .dx-content.docs-changelog
  @h1 .docs-module-section: Date Time Picker Changelog
    @div .docs-module-changelog-link
      @hyperlink /changelog/: Complete Changelog
  @changelog.wrapper
    @process
      @renderSingleItemInRoot
      @inline ../../../../modules/date-time-picker/index.um
    @for v in versionList
      @version {{v}}
        @link /docs/{{v}}/date-time-picker/